    "def fun(x):\n",
    "    s=\"\"\n",
    "    for j in x.keys():\n",
    "        s+=f\"{j}:{x[j]} \"\n",
    "    return s\n",
    "l = [{\"a\":\"b\"},{\"c\":\"d\"},{\"e\":\"f\",\"g\":\"h\"}]\n",
    "m = list(map(fun,l))\n",
    "s = \"\\n\".join(m)\n",
    "print(s)"
   ]
  }

    "def fff(nums,target):\n",
    "    for i in range(len(nums)):\n",
    "        x = nums[i]\n",
    "        nums[i]= 'x'\n",
    "        dif = target - x\n",
    "        if dif in nums:\n",
    "            return(i,nums.index(dif))\n",
    "nums = [3,2,4,5,3]\n",
    "target = 6\n",
    "print(fff(nums,target))"
